class Solution {
    public int removeDuplicates(int[] nums) {
        int len = nums.length;
        int duplicate = 0;
        for(int index = 0 ; index < len ; index++){
            if(nums[index] != nums[duplicate]){
                nums[duplicate+1] = nums[index];
                duuplicate++;
            }
        }
        return duplicate + 1;
    }
}
